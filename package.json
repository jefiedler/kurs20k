{
  "name": "kurs20k",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "description": "This is my journeyman piece in the bootcamp of neuefische",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && npm run build",
    "prettify": "prettier --write .",
    "server": "nodemon server.js",
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefiedler/kurs20k.git"
  },
  "author": "Jonas Elias Fiedler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jefiedler/kurs20k/issues"
  },
  "homepage": "https://github.com/jefiedler/kurs20k#readme",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json}": "prettier --write"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
